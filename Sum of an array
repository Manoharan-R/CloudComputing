#define a function 
def calculate_sum(array):
#intialize sum
   sum=0
   for num in array:
       sum += num
   return sum
    
n=int(input("Enter the number of elements in an array:"))
array=[]
for i in range(n):
    num = int(input("Enter element {}: ".format(i+1)))
    array.append(num)
result = calculate_sum(array)
#print the result of the sum
print("The sum of the given numbers is:", result)
